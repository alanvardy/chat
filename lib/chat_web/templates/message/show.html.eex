<h1>Show Message</h1>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @message.name %>
  </li>

  <li>
    <strong>Content:</strong>
    <%= @message.content %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.message_path(@conn, :edit, @message) %></span>
<span><%= link "Back", to: Routes.message_path(@conn, :index) %></span>
